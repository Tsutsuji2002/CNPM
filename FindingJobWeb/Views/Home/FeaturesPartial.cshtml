@model IEnumerable<FindingJobWeb.Models.Feature_area>

<!-- Start features Area -->
<section class="features-area">
    <div class="container">
        <div class="row">
            @foreach (var fa in Model)
            {
                <div class="col-lg-3 col-md-6">
                    <div class="single-feature">
                        <h4>@fa.Name</h4>
                        <p>
                            @fa.Description
                        </p>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
<!-- End features Area -->
