@model IEnumerable<FindingJobWeb.Models.JobsbyLocation>

<div class="single-slidebar">
    <h4>Công việc theo địa điểm</h4>
    <ul class="cat-list">
        @foreach (var l in Model)
        {
            <li><a class="justify-content-between d-flex" href="@Url.Action("JobbyLocation","Job", new { id = l.LocationID , LocationName = l.Name})"><p>@l.Name</p><span>@l.NoOfJobs</span></a></li>
        }
    </ul>
</div>
